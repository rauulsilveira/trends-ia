{
  "name": "backend",
  "version": "1.0.0",
  "type": "module",
  "main": "src/app.ts",
  "scripts": {
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "start": "node --loader ts-node/esm src/app.ts",
    "seed": "node --loader ts-node/esm prisma/seed.ts",
    "dev": "npm run seed && npm run start",
    "dev:watch": "nodemon --watch 'src/**/*.ts' --exec 'node --loader ts-node/esm' src/app.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.16.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.1",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "prisma": "^6.16.1"
  }
}
