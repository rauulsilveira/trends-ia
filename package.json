{
  "name": "trends-ia",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm start",
    "crawl:google": "cd backend && npm run crawl:google",
    "worker": "cd backend && npm run worker",
    "dev:full": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\" \"npm run worker\"",
    "setup": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "build": "cd backend && npm run build && cd ../frontend && npm run build",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.16.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "concurrently": "^8.0.0",
    "prisma": "^6.16.2"
  }
}
