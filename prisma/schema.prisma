generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Trend {
  id        Int     @id @default(autoincrement())
  title     String
  summary   String
  url       String?       // <- aqui Ã© importante que exista
  thumbnail String?
  tags      String        // JSON string
}


